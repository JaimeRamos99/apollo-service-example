{
  "name": "pure-clone",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "dev-strict": "concurrently \"types-check -w\" \"npm run watch-compile\" \"npm run watch-dev\"",
    "types-check": "tsc --pretty --skipLibCheck --noEmit",
    "watch-compile": "swc src -w --out-dir build",
    "watch-dev": "nodemon --watch \"build/**/*\" -e js ./build/index.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.9.0",
    "graphql": "^16.7.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.72",
    "@types/node": "^20.4.5",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  }
}
